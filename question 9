#include <stdio.h>

int main() {
    int rows, cols, i, j;
    int matrix[10][10];

    printf("Enter the number of rows: ");
    scanf("%d", &rows);

    printf("Enter the number of columns: ");
    scanf("%d", &cols);

    // Input the elements of the matrix
    printf("Enter the elements of the matrix:\n");
    for(i = 0; i < rows; i++) {
        for(j = 0; j < cols; j++) {
            scanf("%d", &matrix[i][j]);
        }
    }

    // Check if the matrix is an identity matrix
    int is_identity = 1;
    for(i = 0; i < rows; i++) {
        for(j = 0; j < cols; j++) {
            if(i == j) {
                if(matrix[i][j] != 1) {
                    is_identity = 0;
                    break;
                }
            } else {
                if(matrix[i][j] != 0) {
                    is_identity = 0;
                    break;
                }
            }
        }
    }

    // Print the matrix and whether it is an identity matrix
    if(is_identity) {
        printf("The matrix is:\n");
        for(i = 0; i < rows; i++) {
            for(j = 0; j < cols; j++) {
                printf("%d ", matrix[i][j]);
            }
            printf("\n");
        }
        printf("The matrix is an identity matrix.\n");
    } else {
        printf("The matrix is not an identity matrix.\n");
    }

    return 0;
}
